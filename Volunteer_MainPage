package com.example.galaxycare.BackendCode.EVERYTHING;

import java.util.Scanner;

public class Volunteer_Page {

    private static boolean volunteer;

    public static void main(String args[]) {
        Scanner sc = new Scanner(System.in);

        System.out.println("Would you like to be a volunteer? ");
        if (sc.next().toLowerCase() == "yes") volunteer = true;
        else volunteer = false;

        System.out.println("Login or Sign up? ");
        if (sc.next().toLowerCase() == "login") loginPage();
        else {
            if (volunteer) new Volunteer_Page();
            else new Patient_Page();
        }
    }

}
